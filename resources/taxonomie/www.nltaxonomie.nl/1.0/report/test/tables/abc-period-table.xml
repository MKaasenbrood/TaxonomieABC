<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<link:linkbase 
	xmlns:gf="http://xbrl.org/2008/filter/general"
	xmlns:variable="http://xbrl.org/2008/variable" 
	xmlns:cf="http://xbrl.org/2008/filter/concept" 	
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:label="http://xbrl.org/2008/label" 
	xmlns:gen="http://xbrl.org/2008/generic"
	xmlns:gpl="http://xbrl.org/2013/preferred-label" 
	xmlns:xlink="http://www.w3.org/1999/xlink"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:link="http://www.xbrl.org/2003/linkbase"
	xmlns:table="http://xbrl.org/2014/table"
	xmlns:formula="http://xbrl.org/2008/formula"
	xmlns:abc-abstr="http://www.nltaxonomie.nl/report/test/abstracts/test-abstracts"
	xmlns:test-axes="http://www.nltaxonomie.nl/domein/test/axes/test-axes"
    xmlns:test-doms="http://www.nltaxonomie.nl/basis/test/domains/test-domains"
		xsi:schemaLocation="             
		http://www.xbrl.org/2003/linkbase http://www.xbrl.org/2003/xbrl-linkbase-2003-12-31.xsd
		http://xbrl.org/2008/generic http://www.xbrl.org/2008/generic-link.xsd
		http://xbrl.org/2008/label http://www.xbrl.org/2008/generic-label.xsd
		http://xbrl.org/2013/preferred-label http://www.xbrl.org/2013/preferred-label.xsd
		http://xbrl.org/2014/table http://www.xbrl.org/2014/table.xsd">


	<link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2014/table-breakdown" xlink:type="simple" xlink:href="http://www.xbrl.org/2014/table.xsd#table-breakdown"/>
	<link:arcroleRef arcroleURI="http://xbrl.org/arcrole/2014/breakdown-tree" xlink:type="simple" xlink:href="http://www.xbrl.org/2014/table.xsd#breakdown-tree"/>

	<gen:link xlink:type="extended" xlink:role="http://www.xbrl.org/2003/role/link">
		<table:table xlink:type="resource" xlink:label="abc-table_AbcPeriod" id="abc-table_AbcPeriod"/>

		<!--X Axis Dimension breakdown definition-->
		<table:breakdown parentChildOrder="parent-first" xlink:type="resource" xlink:label="abc-table_AbcPeriod-x-dim" id="abc-table_AbcPeriod-x-dim"/>
		<table:tableBreakdownArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2014/table-breakdown" axis="x" xlink:from="abc-table_AbcPeriod" xlink:to="abc-table_AbcPeriod-x-dim" order="0"/>
		<table:ruleNode xlink:type="resource" xlink:label="abc-table_AbcPeriod-x.0" id="abc-table_AbcPeriod-x.0">
			<formula:explicitDimension dimension="test-axes:DimensionA">
				<formula:member>
					<formula:qname>test-doms:MemberA</formula:qname>
				</formula:member>
			</formula:explicitDimension>
		</table:ruleNode>
		<table:breakdownTreeArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2014/breakdown-tree" xlink:from="abc-table_AbcPeriod-x-dim" xlink:to="abc-table_AbcPeriod-x.0" order="1"/>

		<!--X Axis Period breakdown definition-->
		<table:breakdown parentChildOrder="parent-first" xlink:type="resource" xlink:label="abc-table_AbcPeriod-x" id="abc-table_AbcPeriod-x"/>
		<table:tableBreakdownArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2014/table-breakdown" axis="x" xlink:from="abc-table_AbcPeriod" xlink:to="abc-table_AbcPeriod-x" order="1"/>
		<table:ruleNode xlink:type="resource" xlink:label="abc-table_AbcPeriod-x.1" id="abc-table_AbcPeriod-x.1">
			<formula:period>
				<formula:instant value="xs:date('2015-12-31')" />
			</formula:period>
		</table:ruleNode>
		<table:breakdownTreeArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2014/breakdown-tree" xlink:from="abc-table_AbcPeriod-x" xlink:to="abc-table_AbcPeriod-x.1" order="1"/>


		<!--Y Axis breakdown definition-->
		<table:breakdown parentChildOrder="parent-first" xlink:type="resource" xlink:label="abc-table_AbcPeriod-y" id="abc-table_AbcPeriod-y"/>
		<table:tableBreakdownArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2014/table-breakdown" axis="y" xlink:from="abc-table_AbcPeriod" xlink:to="abc-table_AbcPeriod-y" order="1"/>
		<table:conceptRelationshipNode xlink:type="resource" xlink:label="abc-table_AbcPeriod-y.1" id="abc-table_AbcPeriod-y.1">
			<table:relationshipSource>abc-abstr:TitleA</table:relationshipSource>
			<table:linkrole>urn:test:linkrole:a</table:linkrole>
			<table:arcrole>http://www.xbrl.org/2003/arcrole/parent-child</table:arcrole>
			<table:formulaAxis>descendant-or-self</table:formulaAxis>
		</table:conceptRelationshipNode>
		<table:breakdownTreeArc xlink:type="arc" xlink:arcrole="http://xbrl.org/arcrole/2014/breakdown-tree" xlink:from="abc-table_AbcPeriod-y" xlink:to="abc-table_AbcPeriod-y.1" order="1"/>

	</gen:link>

</link:linkbase>